@using EntityLayer.Concreate
@model List<Contact>
@{ 
    <h2>İletişim Listesi</h2>
    }
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <h2>Kategori Listeleme</h2>

    <table class="table table-hover">
        <tr>
            <td>Contact ID</td>
            <td>Contact Adı</td>
            <td>Contact Açıklaması</td>
            

        </tr>
        @foreach (var item in Model)
        {
            <tr>

                <td>@item.ContactID</td>
                <td>@item.Username</td>
                <td>@item.Usermail</td>
                



            </tr>
}
    </table>

    <a href="/Contact/AddContact">Yeni Contact</a>